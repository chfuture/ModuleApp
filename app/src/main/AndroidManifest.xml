<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ycwang.moduleapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".global.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="theme,label,icon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".fragment.view.activity.ViewPagerActivity" />
        <activity android:name=".annotation.AnnotationActivity" />
        <activity android:name=".dispatchEvent.activity.view.activity.ModuleMainActivity" />
        <activity android:name=".service.ServiceActivity" />
        <activity android:name=".room.RoomActivity" />
        <activity android:name=".easyPermission.PermissionActivity1" />
        <service android:name=".service.LocalService1" />
    </application>

</manifest>